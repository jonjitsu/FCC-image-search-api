{
  "name": "image-search-microservice",
  "version": "0.1.0",
  "description": "Image search API microservice",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec babel-node --presets es2015,stage-2",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir -p dist && babel src -d dist",
    "test": "mocha --recursive --compilers js:babel-register",
    "test:watch": "fsmonitor -s '+src/*.js' '+test/*.js' npm test"
  },
  "author": "Jonathan Willis",
  "license": "",
  "devDependencies": {
    "babel-cli": "^6.3.15",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "fsmonitor": "^0.2.4",
    "mocha": "^2.3.4",
    "node-inspector": "^0.12.5",
    "nodemon": "^1.8.1",
    "sinon": "^1.17.3",
    "superagent": "^1.7.1"
  },
  "dependencies": {
    "500px": "^0.5.1",
    "configuror": "^0.1.2",
    "express": "^4.13.4",
    "lodash": "^4.3.0",
    "mongodb": "^2.1.7"
  }
}
